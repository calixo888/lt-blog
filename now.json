{
  "version": 2,
  "name": "nextjs-blog-starter",
  "alias": ["nextjs-blog-starter.now.sh"],
  "builds": [{ "src": "package.json", "use": "@now/next" }],
  "routes": [
    { "src": "/(.*)", "dest": "/$1" },
    { "src": "/feed.json", "dest": "/static/feed.json" },
    { "src": "/blog/(?<page>[^/]+)$", "dest": "/blog?page=$page" }
  ]
}
